<%= form_for @album, html: { multipart: true } do |f| %>
<%= f.label :title %>:
<%= f.text_field :title %><br />

<%= f.label "Project Year:" %>:
<%= f.text_field :project_year %><br />

<%= f.label :location %>:
<%= f.text_field :location %><br />

<%= f.label :price %>:
<%= f.text_field :price %><br />

<%= f.label :description %>:
<%= f.text_area :album_description %><br />

<%= f.label :image %>:
<%= f.file_field :image %><br />

<%= f.submit %>
<% end %>