<% @albums.each do |album|%>
	<%= image_tag album.image.url(:thumb) %> <br>
	<%= album.title %> <br>
	<%= album.location%> <br>
	<%= album.album_description%> <br>
	<%= link_to "Edit Album", edit_album_path(album) %>
	<%= link_to "Delete Album", album_path(album), :method => 'delete' %> <br> <br>
<% end %>