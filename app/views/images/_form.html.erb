<%= form_for :image, url: album_images_path, html: { multipart: true } do |f| %>
<%= f.label :name %>:
<%= f.text_field :name %><br />

<%= f.label :description %>:
<%= f.text_area :description %><br />

<%= f.label :image %>:
<%= f.file_field :image %><br />

<p class= "center-align"><%= f.submit %></p> 
<% end %>